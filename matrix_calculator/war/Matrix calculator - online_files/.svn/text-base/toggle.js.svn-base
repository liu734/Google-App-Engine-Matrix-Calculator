function toggleElem(elemId, managerId) {
	var elem = document.getElementById(elemId);
	var managerHide = document.getElementById(managerId + '-hide');
	var managerShow = document.getElementById(managerId + '-show');
	if (elem.style.display == 'none') {
		elem.style.display = 'block';
		managerShow.style.display = 'none';
		managerHide.style.display = 'inline';
	} else {
		elem.style.display = 'none';
		managerShow.style.display = 'inline';
		managerHide.style.display = 'none';
	}
}

